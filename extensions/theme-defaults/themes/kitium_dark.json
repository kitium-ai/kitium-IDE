		"gitDecoration.deletedResourceForeground": "#EF4444",
		"gitDecoration.untrackedResourceForeground": "#10B981",
		"gitDecoration.addedResourceForeground": "#10B981",
		"gitDecoration.conflictingResourceForeground": "#7C3AED"
	},
	"tokenColors": [
		{
			"name": "Comments",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": { "foreground": "#475569", "fontStyle": "italic" }
		},
		{
			"name": "Keywords",
			"scope": ["keyword", "storage.type", "storage.modifier"],
			"settings": { "foreground": "#3B82F6" }
		},
		{
			"name": "Strings",
			"scope": ["string", "string.quoted"],
			"settings": { "foreground": "#10B981" }
		},
		{
			"name": "Numbers",
			"scope": ["constant.numeric"],
			"settings": { "foreground": "#F59E0B" }
		},
		{
			"name": "Constants",
			"scope": ["constant.language", "constant.character"],
			"settings": { "foreground": "#F59E0B" }
		},
		{
			"name": "Functions",
			"scope": ["entity.name.function", "support.function", "meta.function-call"],
			"settings": { "foreground": "#06B6D4" }
		},
		{
			"name": "Types",
			"scope": ["entity.name.type", "entity.name.class", "support.class", "support.type"],
			"settings": { "foreground": "#A78BFA" }
		},
		{
			"name": "Variables",
			"scope": ["variable", "variable.other"],
			"settings": { "foreground": "#E8EAED" }
		},
		{
			"name": "Parameters",
			"scope": ["variable.parameter"],
			"settings": { "foreground": "#FCD34D", "fontStyle": "italic" }
		},
		{
			"name": "Properties",
			"scope": ["variable.other.property", "support.variable.property"],
			"settings": { "foreground": "#64B5F6" }
		},
		{
			"name": "Operators",
			"scope": ["keyword.operator"],
			"settings": { "foreground": "#06B6D4" }
		},
		{
			"name": "Punctuation",
			"scope": ["punctuation"],
			"settings": { "foreground": "#64748B" }
		},
		{
			"name": "HTML Tags",
			"scope": ["entity.name.tag"],
			"settings": { "foreground": "#3B82F6" }
		},
		{
			"name": "HTML Attributes",
			"scope": ["entity.other.attribute-name"],
			"settings": { "foreground": "#F59E0B" }
		},
		{
			"name": "CSS Properties",
			"scope": ["support.type.property-name.css"],
			"settings": { "foreground": "#06B6D4" }
		},
		{
			"name": "CSS Values",
			"scope": ["support.constant.property-value.css"],
			"settings": { "foreground": "#10B981" }
		},
		{
			"name": "Imports / Modules",
			"scope": ["keyword.control.import", "keyword.control.export", "keyword.control.from"],
			"settings": { "foreground": "#7C3AED" }
		},
		{
			"name": "Decorators",
			"scope": ["meta.decorator", "punctuation.decorator"],
			"settings": { "foreground": "#F59E0B" }
		}
	]
}
